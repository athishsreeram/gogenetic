syntax = "proto3";

package  v1;

import "google/api/annotations.proto";


    
{{range .Models.Model}}
message {{.Name}} {
        {{range .Variable}}
            {{(convertproto .Type)}} {{.Name}} = {{.Value}};
        {{end}}    
}
{{end}}

 

service {{.API.Name}} {
{{range .API.Operations}} 
    rpc {{.Operationid}}({{.Request}}) returns ({{.Response}}){
        option (google.api.http) = {
            {{.Protocol}}: "{{.URL}}"
            body: "*"
        };
    }
{{end}}

}